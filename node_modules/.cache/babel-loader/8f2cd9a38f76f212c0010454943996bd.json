{"ast":null,"code":"import ThemeButton from \"@/components/UI/ThemeButton\";\nimport { mapState } from \"vuex\";\nexport default {\n  components: {\n    ThemeButton\n  },\n\n  data() {\n    return {\n      note: {\n        title: ''\n      }\n    };\n  },\n\n  methods: {\n    createNote() {\n      console.log(this.notes.length);\n      this.note.id = this.notes[this.notes.length - 1].id + 1;\n      console.log(this.notes.length);\n      this.$emit('createNote', this.note);\n      console.log(this.notes.length);\n    }\n\n  },\n  computed: { ...mapState({\n      notes: state => state.notesAndTodos.notes\n    }),\n\n    inputFocus() {\n      return document.querySelector('#note__input').focus();\n    }\n\n  },\n\n  mounted() {\n    this.inputFocus;\n  }\n\n};","map":{"version":3,"mappings":"AAcA,OAAOA,WAAP,MAAwB,6BAAxB;AACA,SAAQC,QAAR,QAAuB,MAAvB;AAEA,eAAe;EACbC,UAAU,EAAE;IAACF;EAAD,CADC;;EAEbG,IAAI,GAAE;IACJ,OAAO;MACLC,IAAI,EAAE;QACJC,KAAK,EAAE;MADH;IADD,CAAP;EAKD,CARY;;EASbC,OAAO,EAAE;IACPC,UAAU,GAAE;MACVC,OAAO,CAACC,GAAR,CAAY,KAAKC,KAAL,CAAWC,MAAvB;MACA,KAAKP,IAAL,CAAUQ,EAAV,GAAe,KAAKF,KAAL,CAAW,KAAKA,KAAL,CAAWC,MAAX,GAAoB,CAA/B,EAAkCC,EAAlC,GAAuC,CAAtD;MACAJ,OAAO,CAACC,GAAR,CAAY,KAAKC,KAAL,CAAWC,MAAvB;MACA,KAAKE,KAAL,CAAW,YAAX,EAAyB,KAAKT,IAA9B;MACAI,OAAO,CAACC,GAAR,CAAY,KAAKC,KAAL,CAAWC,MAAvB;IACF;;EAPO,CATI;EAkBbG,QAAQ,EAAE,EACR,GAAGb,QAAQ,CAAC;MACVS,KAAK,EAAEK,KAAI,IAAKA,KAAK,CAACC,aAAN,CAAoBN;IAD1B,CAAD,CADH;;IAIRO,UAAU,GAAE;MACV,OAAOC,QAAQ,CAACC,aAAT,CAAuB,cAAvB,EAAuCC,KAAvC,EAAP;IACF;;EANQ,CAlBG;;EA0BbC,OAAO,GAAG;IACR,KAAKJ,UAAL;EACF;;AA5Ba,CAAf","names":["ThemeButton","mapState","components","data","note","title","methods","createNote","console","log","notes","length","id","$emit","computed","state","notesAndTodos","inputFocus","document","querySelector","focus","mounted"],"sourceRoot":"","sources":["/home/yegor/projects/vue-test-task/vue-todos/src/components/Forms/FormNote.vue"],"sourcesContent":["<template>\n<div class=\"note\">\n  <div class=\"note__content\">\n    <theme-input\n        id=\"note__input\"\n        placeholder=\"Write a title\"\n        v-model=\"note.title\"\n    />\n    <theme-button style=\"margin-left: 20px\" @click=\"createNote\">Create a note</theme-button>\n  </div>\n</div>\n</template>\n\n<script>\nimport ThemeButton from \"@/components/UI/ThemeButton\";\nimport {mapState} from \"vuex\";\n\nexport default {\n  components: {ThemeButton},\n  data(){\n    return {\n      note: {\n        title: ''\n      }\n    }\n  },\n  methods: {\n    createNote(){\n      console.log(this.notes.length)\n      this.note.id = this.notes[this.notes.length - 1].id + 1\n      console.log(this.notes.length)\n      this.$emit('createNote', this.note)\n      console.log(this.notes.length)\n    }\n  },\n  computed: {\n    ...mapState({\n      notes: state => state.notesAndTodos.notes\n    }),\n    inputFocus(){\n      return document.querySelector('#note__input').focus()\n    }\n  },\n  mounted() {\n    this.inputFocus\n  }\n}\n</script>\n\n<style lang=\"less\">\n@import \"src/assets/styles/variables\";\n\n</style>"]},"metadata":{},"sourceType":"module"}