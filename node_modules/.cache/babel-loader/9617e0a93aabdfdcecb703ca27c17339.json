{"ast":null,"code":"import { mapState } from 'vuex';\nimport FormTodo from \"@/components/Forms/FormTodo\";\nimport TodoItem from \"@/components/Todos/TodoItem\";\nexport default {\n  components: {\n    FormTodo,\n    TodoItem\n  },\n\n  data() {\n    return {\n      id: this.$route.params.id,\n      dialogVisible: false,\n      dialogVisibleTodo: false,\n      getTodo: {}\n    };\n  },\n\n  computed: { ...mapState({\n      notes: state => state.notesAndTodos.notes\n    })\n  },\n  methods: {\n    openForm() {\n      this.dialogVisible = true;\n    },\n\n    createTodo(todo) {\n      this.notes[this.id].todos.push(todo);\n      this.dialogVisible = false;\n    },\n\n    removeTodo(todoId) {\n      this.dialogVisibleTodo = true;\n      this.getTodo = todoId;\n    },\n\n    removeTodo小onfirm() {\n      this.notes[this.id].todos.splice(this.getTodo, 1);\n      this.dialogVisibleTodo = false;\n    }\n\n  },\n\n  mounted() {\n    console.log(this.id);\n  }\n\n};","map":{"version":3,"mappings":"AAmCA,SAAQA,QAAR,QAAuB,MAAvB;AACA,OAAOC,QAAP,MAAqB,6BAArB;AACA,OAAOC,QAAP,MAAqB,6BAArB;AACA,eAAe;EACbC,UAAU,EAAE;IACVF,QADU;IAEVC;EAFU,CADC;;EAKbE,IAAI,GAAE;IACJ,OAAO;MACLC,EAAE,EAAE,KAAKC,MAAL,CAAYC,MAAZ,CAAmBF,EADlB;MAELG,aAAa,EAAE,KAFV;MAGLC,iBAAiB,EAAE,KAHd;MAILC,OAAO,EAAE;IAJJ,CAAP;EAMD,CAZY;;EAabC,QAAQ,EAAE,EACR,GAAGX,QAAQ,CAAC;MACVY,KAAK,EAAEC,KAAI,IAAKA,KAAK,CAACC,aAAN,CAAoBF;IAD1B,CAAD;EADH,CAbG;EAkBbG,OAAO,EAAE;IACPC,QAAQ,GAAE;MACR,KAAKR,aAAL,GAAqB,IAArB;IACD,CAHM;;IAIPS,UAAU,CAACC,IAAD,EAAM;MACd,KAAKN,KAAL,CAAW,KAAKP,EAAhB,EAAoBc,KAApB,CAA0BC,IAA1B,CAA+BF,IAA/B;MACA,KAAKV,aAAL,GAAqB,KAArB;IACD,CAPM;;IAQPa,UAAU,CAACC,MAAD,EAAQ;MAChB,KAAKb,iBAAL,GAAyB,IAAzB;MACA,KAAKC,OAAL,GAAeY,MAAf;IACD,CAXM;;IAYPC,iBAAiB,GAAE;MACjB,KAAKX,KAAL,CAAW,KAAKP,EAAhB,EAAoBc,KAApB,CAA0BK,MAA1B,CAAiC,KAAKd,OAAtC,EAA+C,CAA/C;MACA,KAAKD,iBAAL,GAAyB,KAAzB;IACF;;EAfO,CAlBI;;EAmCbgB,OAAO,GAAG;IACRC,OAAO,CAACC,GAAR,CAAY,KAAKtB,EAAjB;EACF;;AArCa,CAAf","names":["mapState","FormTodo","TodoItem","components","data","id","$route","params","dialogVisible","dialogVisibleTodo","getTodo","computed","notes","state","notesAndTodos","methods","openForm","createTodo","todo","todos","push","removeTodo","todoId","removeTodo小onfirm","splice","mounted","console","log"],"sourceRoot":"","sources":["/home/yegor/projects/vue-test-task/vue-todos/src/pages/NoteItemPage.vue"],"sourcesContent":["<template>\n  <div class=\"note__item__page\">\n    <div class=\"__links\">\n      <theme-button @click=\"$router.push('/')\">Back to NoteList</theme-button>\n    </div>\n    <div class=\"__title\">\n      <strong class=\"note__title\">{{this.notes[this.id].title}}</strong>\n    </div>\n    <div class=\"__todos\">\n      <theme-button id=\"add__todo\" @click=\"openForm\">Add Todo</theme-button>\n      <strong class=\"title-todos\">TODOS:</strong>\n      <TodoItem\n          @removeTodo=\"removeTodo\"\n          v-if=\"this.notes[this.id].todos.length >= 1\"\n          v-for=\"(todo, todoId) in this.notes[this.id].todos\"\n          :todo=\"todo\"\n          :todoId=\"todoId\"\n      />\n      <div v-else>\n        <h3 class=\"else__todos__title\">You need to add todos!</h3>\n      </div>\n    </div>\n\n    <dialog-window v-model:showDialog=\"dialogVisible\">\n        <FormTodo @createTodo=\"createTodo\"/>\n    </dialog-window>\n\n    <dialog-window v-model:showDialog=\"dialogVisibleTodo\" class=\"dialog__remove__todo\">\n      <strong>Confirm todo deletion !!!</strong>\n      <theme-button @click=\"removeTodo小onfirm\">Remove Todo</theme-button>\n    </dialog-window>\n  </div>\n</template>\n\n<script>\nimport {mapState} from 'vuex'\nimport FormTodo from \"@/components/Forms/FormTodo\";\nimport TodoItem from \"@/components/Todos/TodoItem\";\nexport default {\n  components: {\n    FormTodo,\n    TodoItem\n  },\n  data(){\n    return {\n      id: this.$route.params.id,\n      dialogVisible: false,\n      dialogVisibleTodo: false,\n      getTodo: {},\n    }\n  },\n  computed: {\n    ...mapState({\n      notes: state => state.notesAndTodos.notes\n    }),\n  },\n  methods: {\n    openForm(){\n      this.dialogVisible = true\n    },\n    createTodo(todo){\n      this.notes[this.id].todos.push(todo)\n      this.dialogVisible = false\n    },\n    removeTodo(todoId){\n      this.dialogVisibleTodo = true\n      this.getTodo = todoId\n    },\n    removeTodo小onfirm(){\n      this.notes[this.id].todos.splice(this.getTodo, 1)\n      this.dialogVisibleTodo = false\n    }\n  },\n  mounted() {\n    console.log(this.id)\n  }\n}\n</script>\n\n<style lang=\"less\">\n@import \"src/assets/styles/variables\";\n.note__item__page{\n  margin-top: 60px;\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n\n  .dialog__remove__todo{\n    .dialog__content{\n      display: flex;\n      justify-content: space-between;\n      align-items: center;\n      background: #b3ffde !important;\n    }\n  }\n\n  .note__title{\n    font-size: 30px;\n    width: auto;\n    height: auto;\n    padding: 20px 40px;\n    background: @theme-color;\n    color: @theme-color-2;\n    border: 3px solid @theme-color-2;\n    border-radius: 100px;\n    box-shadow: 4px 8px 8px @theme-color-2;\n  }\n\n  .__todos{\n    margin-top: 80px;\n\n    #add__todo{\n      margin-left: 150px;\n    }\n\n    .else__todos__title{\n      color: @theme-color-2;\n      display: flex;\n      justify-content: center;\n      margin-top: 30px;\n    }\n\n    .title-todos{\n      font-size: 25px;\n      display: flex;\n      justify-content: center;\n      align-items: center;\n      width: 100vw;\n      color: @theme-color-2;\n      border-bottom: 2px solid @theme-color-2;\n    }\n  }\n  .__links{\n    margin-bottom: 50px;\n  }\n}\n@media (max-width: 767px) {\n  .note__item__page{\n    .__todos{\n      #add__todo{\n        margin-left: 60px;\n      }\n    }\n  }\n}\n@media (max-width: 425px) {\n  .note__item__page{\n    .note__title{\n      font-size: 16px;\n    }\n    .__todos{\n      #add__todo{\n        margin-left: 20px;\n      }\n    }\n  }\n}\n@media (min-width: 767px) {\n  .note__item__page{\n    .__todos{\n      #add__todo{\n\n      }\n    }\n  }\n}\n</style>"]},"metadata":{},"sourceType":"module"}