{"ast":null,"code":"import NoteItemPage from \"@/pages/NoteItemPage\";\nimport { mapState, mapActions } from 'vuex';\nexport default {\n  components: {\n    NoteItemPage\n  },\n  props: {\n    note: {\n      type: Object\n    },\n    id: Number\n  },\n  methods: { ...mapActions({\n      removeNote: 'notesAndTodos/removeNote'\n    }),\n\n    removeNote2() {\n      this.$emit('remove', this.id);\n    }\n\n  },\n  computed: { ...mapState({\n      notes: state => state.notesAndTodos.notes\n    })\n  },\n\n  mounted() {\n    console.log(this.note, this.id);\n  }\n\n};","map":{"version":3,"mappings":"AAyBA,OAAOA,YAAP,MAAyB,sBAAzB;AACA,SAAQC,QAAR,EAAkBC,UAAlB,QAAmC,MAAnC;AACA,eAAe;EACbC,UAAU,EAAE;IACVH;EADU,CADC;EAIbI,KAAK,EAAC;IACJC,IAAI,EAAE;MACJC,IAAI,EAAEC;IADF,CADF;IAIJC,EAAE,EAAEC;EAJA,CAJO;EAUbC,OAAO,EAAE,EACP,GAAGR,UAAU,CAAC;MACZS,UAAU,EAAE;IADA,CAAD,CADN;;IAIPC,WAAW,GAAE;MACX,KAAKC,KAAL,CAAW,QAAX,EAAqB,KAAKL,EAA1B;IACF;;EANO,CAVI;EAkBbM,QAAQ,EAAC,EACP,GAAGb,QAAQ,CAAC;MACVc,KAAK,EAAEC,KAAI,IAAKA,KAAK,CAACC,aAAN,CAAoBF;IAD1B,CAAD;EADJ,CAlBI;;EAuBbG,OAAO,GAAG;IACRC,OAAO,CAACC,GAAR,CAAY,KAAKf,IAAjB,EAAuB,KAAKG,EAA5B;EACF;;AAzBa,CAAf","names":["NoteItemPage","mapState","mapActions","components","props","note","type","Object","id","Number","methods","removeNote","removeNote2","$emit","computed","notes","state","notesAndTodos","mounted","console","log"],"sourceRoot":"","sources":["/home/yegor/projects/vue-test-task/vue-todos/src/components/Notes/NoteItem.vue"],"sourcesContent":["<template>\n  <div class=\"note__item\">\n    <div class=\"note__content\">\n      <strong class=\"note__title\">{{this.note.title}}</strong>\n      <div class=\"note__todos\">\n        <div v-if=\"this.note.todos.length > 0\" class=\"note__todos__content\" v-for=\"todoSliced in this.note.todos.slice(0, 2)\">\n          {{todoSliced.todoTitle}}\n        </div>\n        <div v-else class=\"note__todos__content\">\n          You need to add todos !!!\n        </div>\n        <strong v-if=\"this.note.todos.length > 0\" id=\"more__todos\">. . .</strong>\n      </div>\n      <div class=\"note__btns\">\n\n        <theme-button class=\"note__btn\" v-if=\"this.note.todos.length < 1\" @click=\"$router.push(`/note-item/${this.id}`)\">Add Todos</theme-button>\n        <theme-button class=\"note__btn\" v-if=\"this.note.todos.length > 0\" @click=\"$router.push(`/note-item/${this.id}`)\">Edit</theme-button>\n\n        <theme-button class=\"note__btn\" @click=\"removeNote2\">Remove</theme-button>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport NoteItemPage from \"@/pages/NoteItemPage\";\nimport {mapState, mapActions} from 'vuex'\nexport default {\n  components: {\n    NoteItemPage\n  },\n  props:{\n    note: {\n      type: Object\n    },\n    id: Number\n  },\n  methods: {\n    ...mapActions({\n      removeNote: 'notesAndTodos/removeNote'\n    }),\n    removeNote2(){\n      this.$emit('remove', this.id)\n    }\n  },\n  computed:{\n    ...mapState({\n      notes: state => state.notesAndTodos.notes\n    }),\n  },\n  mounted() {\n    console.log(this.note, this.id)\n  }\n}\n</script>\n\n<style lang=\"less\" scoped>\n@import \"src/assets/styles/variables\";\n.note__item{\n  align-items: center;\n  border: 5px solid @theme-color;\n  margin: 50px 50px 0 50px;\n  border-radius: 13px;\n  box-shadow: 8px 12px 15px @theme-color-2;\n\n  .note__title{\n    color: @theme-color-2;\n    font-size: 25px\n  }\n\n  .note__content{\n    display: flex;\n    justify-content: space-between;\n    align-items: center;\n    margin: 10px 20px 10px 20px;\n\n    .note__btns{\n      .note__btn{\n        margin-right: 20px;\n      }\n    }\n\n    .note__todos{\n      margin: 0 15px 0 15px;\n\n      #more__todos{\n        display: flex;\n        color: @theme-color-2;\n        margin-left: 10px;\n      }\n\n      .note__todos__content{\n        display: flex;\n        color: @theme-color-2;\n        &:last-child{\n          margin-top: 13px;\n        }\n      }\n    }\n  }\n\n  &:hover{\n    filter: brightness(1.2);\n    transition-duration: 0.5s;\n    -webkit-transform: scale(1.2);\n    -ms-transform: scale(1.2);\n    transform: scale(1.2);\n  }\n}\n@media (max-width: 767px) {\n  .note__item{\n    .note__content{\n      .note__btns{\n        display: flex;\n        flex-direction: column;\n\n        .note__btn{\n          margin: 5px;\n        }\n      }\n    }\n  }\n}\n@media (max-width: 425px) {\n  .note__item{\n    margin: 40px 20px 0 20px;\n  }\n}\n@media (max-width: 370px) {\n  .note__item{\n    .note__content{\n      .note__btns{\n        .note__btn\n      }\n    }\n    .note__title{\n      font-size: 18px;\n    }\n  }\n}\n</style>"]},"metadata":{},"sourceType":"module"}