{"ast":null,"code":"import NoteList from \"@/components/Notes/NoteList\";\nimport FormNote from \"@/components/Forms/FormNote\";\nimport { mapState, mapMutations, mapActions } from 'vuex';\nexport default {\n  components: {\n    NoteList,\n    FormNote\n  },\n\n  data() {\n    return {\n      dialogVisible: false\n    };\n  },\n\n  methods: { ...mapMutations({\n      setNotes: 'notesAndTodos/setNotes'\n    }),\n    ...mapActions({\n      createNote: 'notesAndTodos/createNote'\n    }),\n\n    openDialog() {\n      this.dialogVisible = true;\n    },\n\n    createNote(note) {\n      this.notes.push(note);\n      this.dialogVisible = false;\n    }\n\n  },\n  computed: { ...mapState({\n      notes: state => state.notesAndTodos.notes\n    })\n  }\n};","map":{"version":3,"mappings":"AAiBA,OAAOA,QAAP,MAAqB,6BAArB;AACA,OAAOC,QAAP,MAAqB,6BAArB;AACA,SAAQC,QAAR,EAAkBC,YAAlB,EAAgCC,UAAhC,QAAiD,MAAjD;AAEA,eAAe;EACbC,UAAU,EAAE;IACVL,QADU;IAEVC;EAFU,CADC;;EAKbK,IAAI,GAAE;IACJ,OAAO;MACLC,aAAa,EAAE;IADV,CAAP;EAGD,CATY;;EAUbC,OAAO,EAAE,EACP,GAAGL,YAAY,CAAC;MACdM,QAAQ,EAAE;IADI,CAAD,CADR;IAIP,GAAGL,UAAU,CAAC;MACZM,UAAU,EAAE;IADA,CAAD,CAJN;;IAOPC,UAAU,GAAE;MACV,KAAKJ,aAAL,GAAqB,IAArB;IACD,CATM;;IAUPG,UAAU,CAACE,IAAD,EAAM;MACd,KAAKC,KAAL,CAAWC,IAAX,CAAgBF,IAAhB;MACA,KAAKL,aAAL,GAAqB,KAArB;IACF;;EAbO,CAVI;EAyBbQ,QAAQ,EAAE,EACR,GAAGb,QAAQ,CAAC;MACVW,KAAK,EAAEG,KAAI,IAAKA,KAAK,CAACC,aAAN,CAAoBJ;IAD1B,CAAD;EADH;AAzBG,CAAf","names":["NoteList","FormNote","mapState","mapMutations","mapActions","components","data","dialogVisible","methods","setNotes","createNote","openDialog","note","notes","push","computed","state","notesAndTodos"],"sourceRoot":"","sources":["/home/yegor/projects/vue-test-task/vue-todos/src/pages/NotesPage.vue"],"sourcesContent":["<template>\n  <div class=\"header\">\n    <div class=\"btns__in__header\">\n      <theme-button class=\"btn__create_note\" @click=\"openDialog\">Create the note</theme-button>\n    </div>\n  </div>\n  <div class=\"note__list\">\n    <note-list />\n  