{"ast":null,"code":"import { mapState } from \"vuex\";\nexport default {\n  data() {\n    return {\n      note: {\n        title: ''\n      }\n    };\n  },\n\n  methods: {\n    createNote() {\n      if (this.notes.length >= 1) {\n        this.note.id = this.notes[this.notes.length - 1].id + 1;\n      } else {\n        this.note.id = 1;\n      }\n\n      this.$emit('createNote', this.note);\n    }\n\n  },\n  computed: { ...mapState({\n      notes: state => state.notesAndTodos.notes\n    }),\n\n    inputFocus() {\n      return document.querySelector('#note__input').focus();\n    }\n\n  },\n\n  mounted() {\n    this.inputFocus;\n  }\n\n};","map":{"version":3,"mappings":"AAcA,SAAQA,QAAR,QAAuB,MAAvB;AAEA,eAAe;EACbC,IAAI,GAAE;IACJ,OAAO;MACLC,IAAI,EAAE;QACJC,KAAK,EAAE;MADH;IADD,CAAP;EAKD,CAPY;;EAQbC,OAAO,EAAE;IACPC,UAAU,GAAE;MACV,IAAG,KAAKC,KAAL,CAAWC,MAAX,IAAqB,CAAxB,EAA0B;QACxB,KAAKL,IAAL,CAAUM,EAAV,GAAe,KAAKF,KAAL,CAAW,KAAKA,KAAL,CAAWC,MAAX,GAAoB,CAA/B,EAAkCC,EAAlC,GAAuC,CAAtD;MACF,CAFA,MAGK;QACH,KAAKN,IAAL,CAAUM,EAAV,GAAe,CAAf;MACF;;MACA,KAAKC,KAAL,CAAW,YAAX,EAAyB,KAAKP,IAA9B;IACF;;EATO,CARI;EAmBbQ,QAAQ,EAAE,EACR,GAAGV,QAAQ,CAAC;MACVM,KAAK,EAAEK,KAAI,IAAKA,KAAK,CAACC,aAAN,CAAoBN;IAD1B,CAAD,CADH;;IAIRO,UAAU,GAAE;MACV,OAAOC,QAAQ,CAACC,aAAT,CAAuB,cAAvB,EAAuCC,KAAvC,EAAP;IACF;;EANQ,CAnBG;;EA2BbC,OAAO,GAAG;IACR,KAAKJ,UAAL;EACF;;AA7Ba,CAAf","names":["mapState","data","note","title","methods","createNote","notes","length","id","$emit","computed","state","notesAndTodos","inputFocus","document","querySelector","focus","mounted"],"sourceRoot":"","sources":["/home/yegor/projects/vue-test-task/vue-todos/src/components/Forms/FormNote.vue"],"sourcesContent":["<template>\n<div class=\"note\">\n  <div class=\"note__content\">\n    <theme-input\n        id=\"note__input\"\n        placeholder=\"Write a title\"\n        v-model=\"note.title\"\n    />\n    <theme-button style=\"margin-left: 20px\" @click=\"createNote\">Create a note</theme-button>\n  </div>\n</div>\n</template>\n\n<scrip