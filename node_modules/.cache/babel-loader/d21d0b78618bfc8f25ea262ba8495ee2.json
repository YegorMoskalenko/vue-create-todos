{"ast":null,"code":"import { mapState } from \"vuex\";\nexport default {\n  data() {\n    return {\n      note: {\n        title: ''\n      }\n    };\n  },\n\n  methods: {\n    createNote() {\n      if (this.notes.length >= 1) {\n        this.note.id = this.notes[this.notes.length - 1].id + 1;\n      } else {\n        this.note.id = 1;\n      }\n\n      this.note.todos = [{\n        id,\n        todoTitle,\n        completed\n      }];\n      this.$emit('createNote', this.note);\n    }\n\n  },\n  computed: { ...mapState({\n      notes: state => state.notesAndTodos.notes\n    }),\n\n    inputFocus() {\n      return document.querySelector('#note__input').focus();\n    }\n\n  },\n\n  mounted() {\n    this.inputFocus;\n  }\n\n};","map":{"version":3,"mappings":"AAcA,SAAQA,QAAR,QAAuB,MAAvB;AAEA,eAAe;EACbC,IAAI,GAAE;IACJ,OAAO;MACLC,IAAI,EAAE;QACJC,KAAK,EAAE;MADH;IADD,CAAP;EAKD,CAPY;;EAQbC,OAAO,EAAE;IACPC,UAAU,GAAE;MACV,IAAG,KAAKC,KAAL,CAAWC,MAAX,IAAqB,CAAxB,EAA0B;QACxB,KAAKL,IAAL,CAAUM,EAAV,GAAe,KAAKF,KAAL,CAAW,KAAKA,KAAL,CAAWC,MAAX,GAAoB,CAA/B,EAAkCC,EAAlC,GAAuC,CAAtD;MACF,CAFA,MAGK;QACH,KAAKN,IAAL,CAAUM,EAAV,GAAe,CAAf;MACF;;MACA,KAAKN,IAAL,CAAUO,KAAV,GAAkB,CAAC;QAACD,EAAD;QAAKE,SAAL;QAAgBC;MAAhB,CAAD,CAAlB;MACA,KAAKC,KAAL,CAAW,YAAX,EAAyB,KAAKV,IAA9B;IACF;;EAVO,CARI;EAoBbW,QAAQ,EAAE,EACR,GAAGb,QAAQ,CAAC;MACVM,KAAK,EAAEQ,KAAI,IAAKA,KAAK,CAACC,aAAN,CAAoBT;IAD1B,CAAD,CADH;;IAIRU,UAAU,GAAE;MACV,OAAOC,QAAQ,CAACC,aAAT,CAAuB,cAAvB,EAAuCC,KAAvC,EAAP;IACF;;EANQ,CApBG;;EA4BbC,OAAO,GAAG;IACR,KAAKJ,UAAL;EACF;;AA9Ba,CAAf","names":["mapState","data","note","title","methods","createNote","notes","length","id","todos","todoTitle","completed","$emit","computed","state","notesAndTodos","inputFocus","document","querySelector","focus","mounted"],"sourceRoot":"","sources":["/home/yegor/projects/vue-test-task/vue-todos/src/components/Forms/FormNote.vue"],"sourcesContent":["<template>\n<div class=\"note\">\n  <div class=\"note__content\">\n    <theme-input\n        id=\"note__input\"\n        p