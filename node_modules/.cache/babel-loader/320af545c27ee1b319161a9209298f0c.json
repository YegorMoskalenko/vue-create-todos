{"ast":null,"code":"import NoteList from \"@/components/Notes/NoteList\";\nimport FormNote from \"@/components/Forms/FormNote\";\nimport { mapState } from 'vuex';\nexport default {\n  components: {\n    NoteList,\n    FormNote\n  },\n\n  data() {\n    return {\n      dialogVisible: false\n    };\n  },\n\n  methods: {\n    openDialog() {\n      this.dialogVisible = true;\n    },\n\n    createNote(note) {\n      this.notes.push(note);\n      this.dialogVisible = false;\n    },\n\n    removeNote(note) {\n      this.notes = this.notes.filter(n => n.id !== note.id);\n    }\n\n  },\n  computed: { ...mapState({\n      notes: state => state.notesAndTodos.notes\n    })\n  }\n};","map":{"version":3,"mappings":"AAiBA,OAAOA,QAAP,MAAqB,6BAArB;AACA,OAAOC,QAAP,MAAqB,6BAArB;AACA,SAAQC,QAAR,QAAuB,MAAvB;AAEA,eAAe;EACbC,UAAU,EAAE;IACVH,QADU;IAEVC;EAFU,CADC;;EAKbG,IAAI,GAAE;IACJ,OAAO;MACLC,aAAa,EAAE;IADV,CAAP;EAGD,CATY;;EAUbC,OAAO,EAAE;IACPC,UAAU,GAAE;MACV,KAAKF,aAAL,GAAqB,IAArB;IACD,CAHM;;IAIPG,UAAU,CAACC,IAAD,EAAM;MACd,KAAKC,KAAL,CAAWC,IAAX,CAAgBF,IAAhB;MACA,KAAKJ,aAAL,GAAqB,KAArB;IACD,CAPM;;IAQPO,UAAU,CAACH,IAAD,EAAM;MACd,KAAKC,KAAL,GAAa,KAAKA,KAAL,CAAWG,MAAX,CAAkBC,KAAKA,CAAC,CAACC,EAAF,KAASN,IAAI,CAACM,EAArC,CAAb;IACF;;EAVO,CAVI;EAsBbC,QAAQ,EAAE,EACR,GAAGd,QAAQ,CAAC;MACVQ,KAAK,EAAEO,KAAI,IAAKA,KAAK,CAACC,aAAN,CAAoBR;IAD1B,CAAD;EADH;AAtBG,CAAf","names":["NoteList","FormNote","mapState","components","data","dialogVisible","methods","openDialog","createNote","note","notes","push","removeNote","filter","n","id","computed","state","notesAndTodos"],"sourceRoot":"","sources":["/home/yegor/projects/vue-test-task/vue-todos/src/pages/NotesPage.vue"],"sourcesContent":["<template>\n  <div class=\"header\">\n    <div class=\"btns__in__header\">\n      <theme-button class=\"btn__create_note\" @click=\"openDialog\">Create the note</theme-button>\n    </div>\n  </div>\n  <div class=\"note__list\">\n    <NoteList @removeNote=\"removeNote\" />\n  </div>\n  <div class=\"create__note__dialog\">\n    <dialog-window v-model:showDialog=\"dialogVisible\">\n      <FormNote @createNote=\"createNote\"/>\n    </dialog-window>\n  </div>\n</template>\n\n<script>\nimport NoteList from \"@/components/Notes/NoteList\";\nimport FormNote from \"@/components/Forms/FormNote\";\nimport {mapState} from 'vuex'\n\nexport default {\n  components: {\n    NoteList,\n    FormNote\n  },\n  data(){\n    return {\n      dialogVisible: false\n    }\n  },\n  methods: {\n    openDialog(){\n      this.dialogVisible = true\n    },\n    createNote(note){\n      this.notes.push(note)\n      this.dialogVisible = false\n    },\n    removeNote(note){\n      this.notes = this.notes.filter(n => n.id !== note.id)\n    }\n  },\n  computed: {\n    ...mapState({\n      notes: state => state.notesAndTodos.notes\n    })\n  }\n}\n</script>\n\n<style lang=\"less\" scoped>\n@import \"src/assets/styles/variables\";\n.header{\n  background: @theme-color;\n  height: 100px;\n  box-shadow: 0 2px 5px @theme-color-2;\n\n  .btns__in__header{\n    height: 100px;\n    display: flex;\n    align-items: center;\n\n    .btn__create_note{\n      margin-left: 50px;\n    }\n  }\n}\n.note__list{\n  display: flex;\n  justify-content: center;\n}\n</style>"]},"metadata":{},"sourceType":"module"}