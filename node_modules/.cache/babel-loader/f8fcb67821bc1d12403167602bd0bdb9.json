{"ast":null,"code":"import { mapState } from 'vuex';\nimport FormTodo from \"@/components/Forms/FormTodo\";\nimport TodoItem from \"@/components/Todos/TodoItem\";\nexport default {\n  components: {\n    FormTodo,\n    TodoItem\n  },\n\n  data() {\n    return {\n      idParam: this.$route.params.id,\n      dialogVisible: false,\n      dialogVisibleTodo: false,\n      getTodo: {}\n    };\n  },\n\n  computed: { ...mapState({\n      notes: state => state.notesAndTodos.notes\n    })\n  },\n  methods: {\n    openForm() {\n      this.dialogVisible = true;\n    },\n\n    createTodo(todo) {\n      this.notes[this.idParam - 1].todos.push(todo);\n      this.dialogVisible = false;\n    },\n\n    removeTodo(todo) {\n      this.dialogVisibleTodo = true;\n      this.getTodo = todo;\n    },\n\n    removeTodo小onfirm() {\n      this.notes[this.idParam - 1].todos = this.notes[this.idParam - 1].todos.filter(t => t.id !== this.getTodo.id);\n      this.dialogVisibleTodo = false;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA6BA,SAAQA,QAAR,QAAuB,MAAvB;AACA,OAAOC,QAAP,MAAqB,6BAArB;AACA,OAAOC,QAAP,MAAqB,6BAArB;AACA,eAAe;EACbC,UAAU,EAAE;IACVF,QADU;IAEVC;EAFU,CADC;;EAKbE,IAAI,GAAE;IACJ,OAAO;MACLC,OAAO,EAAE,KAAKC,MAAL,CAAYC,MAAZ,CAAmBC,EADvB;MAELC,aAAa,EAAE,KAFV;MAGLC,iBAAiB,EAAE,KAHd;MAILC,OAAO,EAAE;IAJJ,CAAP;EAMD,CAZY;;EAabC,QAAQ,EAAE,EACR,GAAGZ,QAAQ,CAAC;MACVa,KAAK,EAAEC,KAAI,IAAKA,KAAK,CAACC,aAAN,CAAoBF;IAD1B,CAAD;EADH,CAbG;EAkBbG,OAAO,EAAE;IACPC,QAAQ,GAAE;MACR,KAAKR,aAAL,GAAqB,IAArB;IACD,CAHM;;IAIPS,UAAU,CAACC,IAAD,EAAM;MACd,KAAKN,KAAL,CAAW,KAAKR,OAAL,GAAe,CAA1B,EAA6Be,KAA7B,CAAmCC,IAAnC,CAAwCF,IAAxC;MACA,KAAKV,aAAL,GAAqB,KAArB;IACD,CAPM;;IAQPa,UAAU,CAACH,IAAD,EAAM;MACd,KAAKT,iBAAL,GAAyB,IAAzB;MACA,KAAKC,OAAL,GAAeQ,IAAf;IACD,CAXM;;IAYPI,iBAAiB,GAAE;MACjB,KAAKV,KAAL,CAAW,KAAKR,OAAL,GAAe,CAA1B,EAA6Be,KAA7B,GAAqC,KAAKP,KAAL,CAAW,KAAKR,OAAL,GAAe,CAA1B,EAA6Be,KAA7B,CAAmCI,MAAnC,CAA0CC,KAAKA,CAAC,CAACjB,EAAF,KAAS,KAAKG,OAAL,CAAaH,EAArE,CAArC;MACA,KAAKE,iBAAL,GAAyB,KAAzB;IACF;;EAfO;AAlBI,CAAf","names":["mapState","FormTodo","TodoItem","components","data","idParam","$route","params","id","dialogVisible","dialogVisibleTodo","getTodo","computed","notes","state","notesAndTodos","methods","openForm","createTodo","todo","todos","push","removeTodo","removeTodo小onfirm","filter","t"],"sourceRoot":"","sources":["/home/yegor/projects/vue-test-task/vue-todos/src/pages/NoteItemPage.vue"],"sourcesContent":["<template>\n  <div class=\"note__item__page\">\n    <div class=\"__links\">\n      <theme-button @click=\"$router.push('/')\">Back to NoteList</theme-button>\n    </div>\n    <div class=\"__title\">\n      <h1>{{this.notes[this.idParam - 1].title}}</h1>\n    </div>\n    <div class=\"__todos\">\n      <theme-button style=\"margin-left: 150px\" @click=\"openForm\">Add Todo</theme-button>\n      <h2>TODOS:</h2>\n      <TodoItem @removeTodo=\"removeTodo\" v-if=\"this.notes[idParam - 1].todos.length < 1\"/>\n      <div v-else>\n        <h3 class=\"else__todos__title\">You need to add todos!</h3>\n      </div>\n    </div>\n\n    <dialog-window v-model:showDialog=\"dialogVisible\">\n        <FormTodo @createTodo=\"createTodo\"/>\n    </dialog-window>\n\n    <dialog-window v-model:showDialog=\"dialogVisibleTodo\" class=\"dialog__remove__todo\">\n      <strong>Confirm todo deletion !!!</strong>\n      <theme-button @click=\"removeTodo小onfirm\">Remove Todo</theme-button>\n    </dialog-window>\n  </div>\n</template>\n\n<script>\nimport {mapState} from 'vuex'\nimport FormTodo from \"@/components/Forms/FormTodo\";\nimport TodoItem from \"@/components/Todos/TodoItem\";\nexport default {\n  components: {\n    FormTodo,\n    TodoItem\n  },\n  data(){\n    return {\n      idParam: this.$route.params.id,\n      dialogVisible: false,\n      dialogVisibleTodo: false,\n      getTodo: {},\n    }\n  },\n  computed: {\n    ...mapState({\n      notes: state => state.notesAndTodos.notes\n    }),\n  },\n  methods: {\n    openForm(){\n      this.dialogVisible = true\n    },\n    createTodo(todo){\n      this.notes[this.idParam - 1].todos.push(todo)\n      this.dialogVisible = false\n    },\n    removeTodo(todo){\n      this.dialogVisibleTodo = true\n      this.getTodo = todo\n    },\n    removeTodo小onfirm(){\n      this.notes[this.idParam - 1].todos = this.notes[this.idParam - 1].todos.filter(t => t.id !== this.getTodo.id)\n      this.dialogVisibleTodo = false\n    }\n  }\n}\n</script>\n\n<style lang=\"less\">\n@import \"src/assets/styles/variables\";\n.note__item__page{\n  margin-top: 60px;\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n\n  .dialog__remove__todo{\n    .dialog__content{\n      display: flex;\n      justify-content: space-between;\n      align-items: center;\n      background: #b3ffde !important;\n    }\n  }\n\n  h1{\n    width: auto;\n    height: auto;\n    padding: 20px 40px;\n    background: @theme-color;\n    color: @theme-color-2;\n    border: 3px solid @theme-color-2;\n    border-radius: 100px;\n    box-shadow: 4px 8px 8px @theme-color-2;\n  }\n\n  .__todos{\n    margin-top: 80px;\n\n    .else__todos__title{\n      color: @theme-color-2;\n      display: flex;\n      justify-content: center;\n      margin-top: 30px;\n    }\n\n    h2{\n      display: flex;\n      justify-content: center;\n      align-items: center;\n      width: 100vw;\n      color: @theme-color-2;\n      border-bottom: 2px solid @theme-color-2;\n    }\n  }\n  .__links{\n    margin-bottom: 50px;\n  }\n}\n</style>"]},"metadata":{},"sourceType":"module"}